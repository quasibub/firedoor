# Frontend Environment Variables

# API Configuration
REACT_APP_API_URL=http://localhost:5000/api

# Authentication
REACT_APP_AUTH_DOMAIN=your-azure-ad-b2c-domain
REACT_APP_CLIENT_ID=your-azure-ad-b2c-client-id
REACT_APP_REDIRECT_URI=http://localhost:3000

# Azure Blob Storage (for file uploads)
REACT_APP_BLOB_STORAGE_URL=your-azure-blob-storage-url
REACT_APP_BLOB_CONTAINER_NAME=fire-door-documents

# Application Insights
REACT_APP_APPINSIGHTS_INSTRUMENTATIONKEY=your-app-insights-key

# Feature Flags
REACT_APP_ENABLE_FILE_UPLOAD=true
REACT_APP_ENABLE_REAL_TIME_UPDATES=false 