{
  "routes": [
    {
      "route": "/*",
      "serve": "/index.html"
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html"
  },
  "headers": [
    {
      "route": "/sw.js",
      "headers": {
        "Cache-Control": "no-cache"
      }
    },
    {
      "route": "/manifest.json",
      "headers": {
        "Cache-Control": "no-cache"
      }
    }
  ]
}
